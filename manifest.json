{
    "name": "Saturday",
    "version": "0.0.3",
    "manifest_version": 2,
    "description": "This extension was created especially for Jesper WÃ¥hlstedt who came up with the concept.",
    "homepage_url": "https://www.niklausgerber.com",
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "default_locale": "en",
    "browser_action": {
        "default_icon": "icons/icon19.png",
        "default_title": "browser action demo"
    },
    "chrome_url_overrides": {
        "newtab": "src/saturday/saturday.html"
    },
    "permissions": [
    "chrome://favicon/"
  ],
    "content_scripts": [
        {
            "matches": [
        "https://www.google.com/*"
      ],
            "css": [
        "src/inject/inject.css"
      ]
    },
        {
            "matches": [
        "https://www.google.com/*"
      ],
            "js": [
        "src/inject/inject.js"
      ]
    }
  ]
}